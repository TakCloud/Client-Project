{
  "extends": "airbnb",
  "root": true,
  "env": {
    'es6': true,
    "node": true,
    "browser": true,
  },
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "jsx-a11y/href-no-hash": "off",
  },
  "globals": {
    "__dirname": true
  },
  "plugins": [
    "react",
    "jsx-a11y",
    "import",
  ],
}
